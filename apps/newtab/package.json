{
  "name": "@cwg/newtab",
  "displayName": "newtab",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:web": "vite",
    "dev:js": "npm run build:js -- --mode development",
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:js",
    "build:prepare": "esno scripts/prepare.ts",
    "build:web": "vite build",
    "build:js": "vite build --config vite.config.content.ts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf extension.zip && jszip-cli add extension/* -o ./extension.zip",
    "pack:crx": "crx pack extension -o ./extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "clear": "rimraf extension/dist extension/manifest.json extension.*",
    "lint": "eslint .",
    "test": "vitest test"
  },
  "dependencies": {
    "@cwg/types": "workspace:*",
    "@cwg/utils": "workspace:*",
    "@meforma/vue-toaster": "^1.3.0",
    "crypto-js": "^4.1.1",
    "element-plus": "^2.2.20",
    "lru-cache": "^7.14.1",
    "lunar-typescript": "^1.2.23",
    "ohash": "^0.1.5",
    "ohmyfetch": "^0.4.21",
    "vee-validate": "^4.7.2",
    "vuedraggable": "^4.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.29.4",
    "@ffflorian/jszip-cli": "^3.1.6",
    "@iconify-json/carbon": "^1.1.10",
    "@iconify/json": "^2.1.136",
    "@types/chrome": "^0.0.200",
    "@types/crypto-js": "^4.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "18.7.18",
    "@types/webextension-polyfill": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@unocss/reset": "^0.46.4",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/test-utils": "^2.2.1",
    "@vueuse/core": "^9.5.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "eslint": "^8.27.0",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.2",
    "kolorist": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "unocss": "^0.46.4",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-icons": "^0.14.13",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.3",
    "vitest": "^0.25.1",
    "vue": "^3.2.45",
    "vue-demi": "^0.13.11",
    "web-ext": "^7.2.0",
    "webext-bridge": "^5.0.5",
    "webextension-polyfill": "^0.10.0"
  }
}
